<div id="episode-container">
    <% for(const episode of episodes){ %>
        <a class="episode" href="/media/player/show/<%= showId %>/<%= episode.id %>">
            <div class="episode-thumbnail" style="background-image: url('<%= episode.still %>');"></div>
            <div class="episode-title">S<%= episode.season %>.E<%= episode.episode %> - <%= episode.title %></div>
            <div class="episode-date">
                <% const months = ['Junly', 'Jan', 'Feb', 'Mar', 'Apr', 'May', 'Jun', 'Jul', 'Aug', 'Sep', 'Oct', 'Nov', 'Dec'] %> 
                <%= months[parseInt(episode.date.toString().slice(4, 6))] %>
                <%= episode.date.toString().slice(-2) %>,
                <%= episode.date.toString().slice(0,4) %></div>
            
        </a>
    <% } %>
</div>